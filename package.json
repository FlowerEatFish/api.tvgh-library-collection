{
  "name": "@flowereatfish/tvgh-library-collection-api",
  "version": "1.1.0",
  "description": "Unofficial Taipei Veterans General Hospital Medical Library Collection API",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "start": "npm run demo:ts",
    "build": "tsc -p ./tsconfig.json",
    "build:dev": "cross-env NODE_ENV=development webpack",
    "build:prod": "cross-env NODE_ENV=production webpack",
    "demo:js": "node ./lib/demo.js",
    "demo:ts": "ts-node ./src/demo.ts",
    "lint": "eslint . -f stylish --ext .js --ext .ts",
    "lint:fix": "eslint . -f stylish --ext .js --ext .ts --fix",
    "test": "jest --coverage --silent"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FlowerEatFish/tvgh-library-collection-api.git"
  },
  "keywords": [
    "api",
    "web-crawler"
  ],
  "author": "Eating-Fish Flower",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FlowerEatFish/tvgh-library-collection-api/issues"
  },
  "homepage": "https://github.com/FlowerEatFish/tvgh-library-collection-api#readme",
  "dependencies": {
    "axios": "^0.19.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/node": "^12.0.8",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "cross-env": "^5.2.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^24.7.1",
    "prettier": "^2.1.2",
    "terser-webpack-plugin": "^5.0.3",
    "ts-jest": "^24.0.1",
    "ts-loader": "^5.4.5",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.1.0"
  }
}
